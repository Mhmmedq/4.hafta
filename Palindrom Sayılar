import java.sql.SQLOutput;

public class mohamm {
    static boolean palidrom(int number){
        int temp= number , resrvnum=0 , ss ;
        while (temp!=0){
            ss=temp%10;
            resrvnum=(resrvnum*10)+ss;
            temp/=10;
        }
        if (resrvnum==number)
            return true;
        else
            return false;
    }


    public static void main(String[] args) {
        System.out.println(palidrom(545));

    }
}

